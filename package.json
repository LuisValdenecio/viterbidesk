{
  "name": "tailwindui-protocol",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:migrate": "cross-env TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex migrate:latest",
    "db:migrate:make": "cross-env TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex migrate:make",
    "db:migrate:undo": "cross-env TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex migrate:down",
    "dev": "next dev",
    "lint": "next lint",
    "lint-and-stage": "npm run prettier && git add .",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "seed": "node -r dotenv/config ./scripts/seed.js",
    "start": "next start",
    "vercel-build": "prisma generate && pnpm db:migrate && next build"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@algolia/autocomplete-core": "^1.7.3",
    "@headlessui/react": "^1.7.15",
    "@headlessui/tailwindcss": "^0.2.0",
    "@heroicons/react": "^2.1.1",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "13.4.16",
    "@prisma/client": "^5.8.1",
    "@sindresorhus/slugify": "^2.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/mdx": "^2.0.8",
    "@types/node": "20.4.7",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "@types/react-highlight-words": "^0.16.4",
    "@vercel/postgres": "^0.5.1",
    "acorn": "^8.8.1",
    "apexcharts": "^3.45.1",
    "autoprefixer": "^10.4.7",
    "aws-sdk": "^2.1554.0",
    "bcrypt": "^5.1.1",
    "clsx": "^1.2.0",
    "fast-glob": "^3.3.0",
    "flexsearch": "^0.7.31",
    "flowbite": "^2.2.1",
    "framer-motion": "7.8.1",
    "knex": "^3.1.0",
    "mdast-util-to-string": "^3.2.0",
    "mdx-annotations": "^0.1.1",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "next-themes": "^0.2.1",
    "papaparse": "^5.4.1",
    "pg": "^8.11.3",
    "react": "18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-dom": "18.2.0",
    "react-highlight-words": "^0.20.0",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-mdx": "^2.3.0",
    "shiki": "^0.11.1",
    "simple-functional-loader": "^1.2.1",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "5.1.6",
    "unist-util-filter": "^4.0.1",
    "unist-util-visit": "^4.1.1",
    "use-debounce": "^10.0.0",
    "zod": "^3.22.2",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@vercel/style-guide": "^5.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.2",
    "prisma": "^5.8.1",
    "sharp": "^0.32.0"
  }
}
